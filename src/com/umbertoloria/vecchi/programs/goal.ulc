int level = 50;
int i = 0;
int a[5] = {1, 3, 2, 5, 6};
int max = 0;
while (i < 5) {
    max+= a[i];
    i++;
}
if (max >= level) {
    print "Array grande";
} else {
    print "Array piccolo";
}


// RULES
a  < b  // low a b
a <= b  // low b a (negata)
a  > b  // gre a b
a >= b  // gre b a (negata)
a == b  // equ a b
a != b  // equ a b (negata)